    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "5.2.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "rimraf": "2.5.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
